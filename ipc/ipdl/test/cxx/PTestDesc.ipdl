include protocol PTestDescSub;
include protocol PTestDescSubsub;

namespace mozilla {
namespace _ipdltest {

protocol PTestDesc {
    manages PTestDescSub; 
child:
    PTestDescSub();

    Test(PTestDescSubsub a);

parent:
    Ok(PTestDescSubsub a);


state START:
    send Test goto ACK;

state ACK:
    recv Ok goto ACK;
    // delete
};

}
}
